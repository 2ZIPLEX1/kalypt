{
  "name": "solana-pump-bundler-bot",
  "version": "1.0.0",
  "description": "Advanced Solana token bundler for Pump.Fun with Telegram bot interface",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:bot": "ts-node src/bot/index.ts",
    "db:test": "ts-node src/test-db.ts",
    "db:migrate": "ts-node src/db/migrations/run.ts",
    "test": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "solana",
    "pump.fun",
    "bundler",
    "jito",
    "telegram-bot",
    "token-launch"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@jup-ag/api": "^6.0.47",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.95.8",
    "axios": "^1.7.9",
    "bn.js": "^5.2.1",
    "borsh": "^2.0.0",
    "bs58": "^6.0.0",
    "bullmq": "^5.28.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "form-data": "^4.0.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jito-ts": "^2.2.0",
    "joi": "^17.13.3",
    "pg": "^8.13.1",
    "telegraf": "^4.16.3",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.6",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
